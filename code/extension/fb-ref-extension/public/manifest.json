{
    "name": "FB-REF-EXTENSION",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "tools for fbref",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["./main.js"],
        "css": ["/main.css"]
      }
    ],
    "browser_action": {
      "default_popup": "./popup.html"
    },
    "permissions": ["storage"]
  }